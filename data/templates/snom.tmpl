        {# //// verify: already present in <phone-settings> #}
        <firmware perm="">{{ firmware_url }}</firmware>
        <firmware_uxm perm=""></firmware_uxm>
    </firmware-settings>
    <functionKeys>
        {# line keys #}
        {% if cap_linekey_count > 0 %}
            {% for number in 1..(cap_linekey_count) %}
                {% if _context['linekey_type_' ~ number] is defined %}
                    <fkey idx="{{ (number-1) }}" context="active" label="{{ _context['linekey_label_' ~ number] }}" lp="on" perm="">{{snom.linekey_type_map(_context['linekey_type_' ~ number], _context['linekey_value_' ~ number], _context['dndtoggle'], _context['pickup_group'])}}</fkey>
                {% else %}
                    <fkey idx="{{ (number-1) }}" context="active" label="" lp="on" perm="">line</fkey>
                {% endif %}
            {% endfor %}
        {% endif %}

        {# expansion keys #}
        {% for number in lineKeys..125 %}
            {% if _context['expkey_type_' ~ number] is defined %}
                <fkey idx="{{ number }}" context="active" label="{{ _context['expkey_label_' ~ number] }}" lp="on" default_text="$name" perm="">{{ _context['expkey_type_' ~ number] }} {{ _context['expkey_value_' ~ number] }}</fkey>
            {% endif %}
        {% endfor %}
    </functionKeys>
{% endautoescape %}
</settings>
