{% import 'snom.macros' as snom %}
<?xml version="1.0" encoding="utf-8"?>
<settings>
{% autoescape %}
    <phone-settings>
        <language perm="">{{ snom.language(language) }}</language>
        {% if default_ringtone == 0 %}
            <ring_sound perm="">Silent</ring_sound>
        {% elseif default_ringtone <= 10 %}
            <ring_sound perm="">Ringer{{ default_ringtone }}</ring_sound>
        {% else %}
            <ring_sound perm="">Ringer1</ring_sound>
        {% endif %}

        <auto_redial perm="">{{ auto_redial ? 'on' : 'off' }}</auto_redial>
        <vlan_id perm="">{{ voice_vlan_enable ? voice_vlan_id : '' }}</vlan_id>
        <vlan_pc_id perm="">{{ data_vlan_enable ? data_vlan_id : '' }}</vlan_pc_id>
        <vlan_qos perm="">{{ voice_vlan_priority }}</vlan_qos>

        {# //// verify #}
        <vlan_port_tagging perm="">{{ data_vlan_enable ? 'on' : 'off' }}</vlan_port_tagging>

        {# //// verify #}
        <vlan_pc_priority perm=""></vlan_pc_priority>

        <ntp_server perm="">{{ ntp_server ?: 'pool.ntp.org' }}</ntp_server>
        <date_us_format perm="">{{ date_format in [0, 2, 4] ? 'on' : 'off' }}</date_us_format>
        <time_24_format perm="">{{ time_format ? 'on' : 'off' }}</time_24_format>
        <ldap_base perm="">{{ ldap_base }}</ldap_base>
        <ldap_port perm="">{{ ldap_port }}</ldap_port>
        <ldap_server perm="">{{ ldap_server | default(hostname) }}</ldap_server>
        <ldap_username perm="">{{ ldap_username }}</ldap_username>
        <ldap_password perm="">{{ ldap_password }}</ldap_password>
        <ldap_over_tls perm="">{{ ldap_tls ? 'on' : 'off' }}</ldap_over_tls>
        <ldap_display_name perm="">{{ ldap_display_name }}</ldap_display_name>
        <ldap_number_attributes perm="">{{ ldap_number_attr }}</ldap_number_attributes>
        <ldap_name_attributes perm="">{{ ldap_name_attr }}</ldap_name_attributes>
        <ldap_number_filter perm="">{{ ldap_number_filter }}</ldap_number_filter>
        <ldap_search_filter perm="">{{ ldap_name_filter }}</ldap_search_filter>

        {% if backlight_time == 0 %}
            <dim_timer perm="">4294967296</dim_timer>
        {% elseif backlight_time == 1 %}
            <dim_timer perm="">0</dim_timer>
        {% else %}
            <dim_timer perm="">{{ backlight_time }}</dim_timer>
        {% endif %}

        <screen_saver_timeout perm="">{{ screensaver_wait_time }}</screen_saver_timeout>
        <custom_bg_image_url perm="">{{ wallpaper_url }}</custom_bg_image_url>
        <dhcp perm="">{{ dhcp_enable ? 'on' : 'off' }}</dhcp>
        <dhcp_v6 perm="">off</dhcp_v6>

        {% if dss_transfer == 1 %}
            <transfer_dialing_on_transfer perm="">attended</transfer_dialing_on_transfer>
        {% else %}
            {# default value #}
            <transfer_dialing_on_transfer perm="">blind</transfer_dialing_on_transfer>
        {% endif %}

        <admin_mode_password perm="">{{ adminpw }}</admin_mode_password>
        <admin_mode_password_confirm perm="">{{ adminpw }}</admin_mode_password_confirm>
        <http_user perm="">admin</http_user>
        <http_password perm="">{{ adminpw }}</http_password>

        <setting_server perm="RW">{{ provisioning_url2 }}</setting_server>
        {# snom allowed values: 3..15. active_backlight_level values: 1..10 #}
        <backlight perm="">{{ active_backlight_level + 5 }}</backlight>
        {# snom allowed values: 0..15. inactive_backlight_level values: 0 (off), 1 (low) #}
        <backlight_idle perm="">{{ inactive_backlight_level * 15 }}</backlight_idle>
        <timezone perm="">{{ snom.timezone(timezone) }}</timezone>
        <tone_scheme perm="">{{ snom.tonezone(tonezone) }}</tone_scheme>
