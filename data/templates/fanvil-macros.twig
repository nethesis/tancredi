#
# Translate generic strings
#
{% macro gettext(txt, lang) %}
    {%- set l10n = {
        'Italian': {
            'Phonebook': 'Rubrica'
        }
    } -%}
    {% if l10n[lang] is defined %}
        {{- l10n[lang][txt] ?? txt -}}
    {% else %}
        {{- txt -}}
    {% endif %}
{% endmacro gettext %}

